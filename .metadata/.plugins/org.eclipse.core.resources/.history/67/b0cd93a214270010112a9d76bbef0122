package com.example.demo.Model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Exams {
	
	
	private  int exame_id;
	 private  int teacher_id;
//	 public String Subject;
	
	 private  String exam_name;
	 private  String date ;
	private  int duration_minuets;
	 private  int total_marks;
	 private  int passing_marks;
	private  String status;
	@Override
	public String toString() {
		return "Exams [exame_id=" + exame_id + ", subject_id=" + subject_id + ", teacher_id=" + teacher_id
				+ ", exam_name=" + exam_name + ", date=" + date + ", duration_minuets=" + duration_minuets
				+ ", total_marks=" + total_marks + ", passing_marks=" + passing_marks + ", status=" + status + "]";
	}
	
	 

}
