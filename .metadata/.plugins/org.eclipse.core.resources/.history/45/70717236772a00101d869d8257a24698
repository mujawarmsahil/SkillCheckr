public class CreatExamsController {
	
	@Autowired
	public AddExamsService createExamServ;
	
	@PostMapping("/addQues")
	public ResponseEntity<String> addAllQuestion(@RequestBody List<QuestionsDT> questions)
	{
		createExamServ.
		
		 return  null;
	}
	

}
